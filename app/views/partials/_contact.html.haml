.our-contact.row
  .contact-us
    .call-us.col.s12.m6.l3
      .cover-blind
        %i.fa.fa-phone-square
        %h4.text-center Call us
        %a{:href => "#"} 
          %h6.text-center 02076352679, 07853292983
          %p.text-center one of our team will answer your call
    .email-us.col.s12.m6.l3
      .cover-blind
        %i.fa.fa-envelope-square
        %h4.text-center Send Email
        %a{:href => ""} 
          %h6.text-center design@miscotech.co.uk
          %p.text-center we will reply your email quickly
    .tweet-us.col.s12.m6.l3
      .cover-blind
        %i.fa.fa-twitter-square
        %h4.text-center Tweet us
        %a{:href => "#"} 
          %h6.text-center @onklickmedia
          %p.text-center we will definitely tweet you back
    .facebook.col.s12.m6.l3
      .cover-blind
        %i.fa.fa-facebook-square
        %h4.text-center Find us
        %a{:href => "https://www.facebook.com/onklickmedia", target: 'blank'} 
          %h6.text-center Onklick media
          %p.text-center you can also message us on facebook 
